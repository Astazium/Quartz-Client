
<container size="600,380" color='#0F1E2DB2' interval="1" context="menu">
    <panel id="player_list" pos='230,15' size='355,380' color='0' max-length='365' scrollable='true'>
        <!-- content is generated in script -->
    </panel>

    <image id="sad" gravity="center-right" margin="0,0,125,0" src='gui/sad' size='128'/>

    <label id="online" gravity="bottom-left" margin="15,0,0,15">Online: 0/0</label>
    <label id="ping" gravity="bottom-left" margin="15,0,0,40">Ping: -1ms</label>
    <label id="pid" gravity="bottom-left" margin="15,0,0,65">PID: -1</label>

    <panel pos="15,15" size="200,380" color="#00000000" interval="30">
        <button onclick='menu:reset()'>@Continue</button>
        <button onclick='menu.page="settings"'>@Settings</button>
        <button onclick='leave()'>@Save and Quit to Menu</button>
    </panel>
</container>
